{
  "name": "mfast",
  "version": "2.0.8",
  "private": true,
  "scripts": {
    "bundle:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "android": "cd android && ./gradlew clean && cd .. && react-native run-android --variant=developDebug",
    "a": "react-native run-android --variant=developDebug",
    "ar": "react-native run-android --variant=release",
    "ios": "react-native run-ios --simulator='iPhone 14 Pro'",
    "start": "react-native start",
    "test": "jest __tests__",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "codepush:android:dev": "appcenter codepush release-react -a phuong.nguyennam-digipaysolution.com/MFast-DEV -d Development",
    "codepush:android:prod": "appcenter codepush release-react -a phuong.nguyennam-digipaysolution.com/MFast-DEV -d Production",
    "codepush:android:staging": "appcenter codepush release-react -a phuong.nguyennam-digipaysolution.com/MFast-DEV -d Staging",
    "codepush:ios:dev": "appcenter codepush release-react -a phuong.nguyennam-digipaysolution.com/MFast-Dev-iOS -d Development",
    "codepush:ios:prod": "appcenter codepush release-react -a phuong.nguyennam-digipaysolution.com/MFast-Dev-iOS -d Production",
    "codepush:ios:staging": "appcenter codepush release-react -a phuong.nguyennam-digipaysolution.com/MFast-Dev-iOS -d Staging",
    "config:android": "python3 0-scripts/configure_env_android.py",
    "config:ios": "python3 0-scripts/configure_env_ios.py",
    "config:all": "python3 0-scripts/configure_env_ios.py && python3 0-scripts/configure_env_android.py",
    "ios:first": "rm -rf node_modules && yarn install && cd ios && pod install && yarn ios",
    "node:clean": "rm -rf node_modules && rm -rf yarn.lock && yarn install && yarn jetify",
    "flutter:android": "cd forked_node_modules/flutter_module && flutter build aar --no-tree-shake-icons --no-debug --no-profile && cd ../..",
    "flutter:ios": "cd forked_node_modules/flutter_module && flutter pub get && cd ../..",
    "android:build": "yarn flutter:android && yarn node:clean && yarn android",
    "ios:build": "yarn node:clean && cd ios && pod install && yarn ios",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:release:develop": "yarn flutter:android && yarn node:clean && cd android && ./gradlew clean && ./gradlew wrapper && ./gradlew assembleDevelopRelease && cd ..",
    "apk": "yarn flutter:android && yarn install && yarn jetify && yarn config:android && ./prepare_build.sh && cd android && ./gradlew wrapper && ./gradlew assembleStagingRelease",
    "submodules:remove": "cd app && rm -rf submodules && cd ..",
    "submodules:repair": "git submodule update --init --recursive && cd app/submodules/firebase && cp NativeUtil.mobile.js NativeUtil.js && cd ../../..",
    "submodules:clean": "yarn submodules:remove && yarn submodules:repair"
  },
